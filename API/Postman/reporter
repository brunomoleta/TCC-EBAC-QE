<!-- Set slider initial position depending on the localstorage state -->

<script>
(function () {
  var sliderChecked = true;
  if (localStorage.getItem('theme') === 'theme-light') {
    setTheme('theme-light');
    sliderChecked = false;
  } else {
    setTheme('theme-dark');
    sliderChecked = true;
  }
  $(document).ready( function () {
    document.getElementById('slider').checked = sliderChecked;
  });
})();
</script>

<!-- Data Table Configuration -->

<script>
$(document).ready( function () {
    $('.datatable').DataTable({
        "retrieve": true,
        "paging": false,
        "info": false,
        "fixedColumns":   {
            "heightMatch": 'none'
        }
    });
});
</script>

<!-- Tooltip Configuration -->

<script>
$(document).ready(function() {
    $('[data-toggle="tooltip"]').tooltip({
        trigger : 'hover'
    })
})
</script>

<!-- Show/Hide The Folders -->

<script>
$('#openAll').on('click', function(e) {
let clickCount = $(this).data("clickCount") || 1
switch (clickCount){
    case 1:
            $('#folder-ddc983bd-e033-4814-9269-40c004f61f38-iteration-0').removeClass('collapsed')
            $('#folder-collapse-ddc983bd-e033-4814-9269-40c004f61f38-iteration-0').addClass('show')
        $('#openAll').html("Collapse Folders");
        break;
    case 2:
            $('#folder-ddc983bd-e033-4814-9269-40c004f61f38-iteration-0').addClass('collapsed')
            $('#folder-collapse-ddc983bd-e033-4814-9269-40c004f61f38-iteration-0').removeClass('show')
        $('#openAll').html("Expand Folders");
        break;
}
clickCount = clickCount > 1 ? 1 : ++clickCount;
$(this).data("clickCount", clickCount)
})
</script>

<!-- Show/Hide The Requests -->

<script>
$('#openAllRequests').on('click', function(e) {
let clickCount = $(this).data("clickCount") || 1
switch (clickCount){
    case 1:
            $('#requests-ccc94b97-86b6-48cc-8468-78dab7d39ea2').removeClass('collapsed')
            $('#collapse-ccc94b97-86b6-48cc-8468-78dab7d39ea2').removeClass('collapsed')
            $('#requests-ccc94b97-86b6-48cc-8468-78dab7d39ea2').addClass('show')
            $('#collapse-ccc94b97-86b6-48cc-8468-78dab7d39ea2').addClass('show')
            $('#requests-20a9253c-0533-4055-8a90-7a7af4008c43').removeClass('collapsed')
            $('#collapse-20a9253c-0533-4055-8a90-7a7af4008c43').removeClass('collapsed')
            $('#requests-20a9253c-0533-4055-8a90-7a7af4008c43').addClass('show')
            $('#collapse-20a9253c-0533-4055-8a90-7a7af4008c43').addClass('show')
            $('#requests-fbaf498b-316a-4506-ad0b-1208a38e595e').removeClass('collapsed')
            $('#collapse-fbaf498b-316a-4506-ad0b-1208a38e595e').removeClass('collapsed')
            $('#requests-fbaf498b-316a-4506-ad0b-1208a38e595e').addClass('show')
            $('#collapse-fbaf498b-316a-4506-ad0b-1208a38e595e').addClass('show')
            $('#requests-e9d3b91e-6ddc-4262-8ddc-e382831330ed').removeClass('collapsed')
            $('#collapse-e9d3b91e-6ddc-4262-8ddc-e382831330ed').removeClass('collapsed')
            $('#requests-e9d3b91e-6ddc-4262-8ddc-e382831330ed').addClass('show')
            $('#collapse-e9d3b91e-6ddc-4262-8ddc-e382831330ed').addClass('show')
